{
  "name": "iot-central-bridge",
  "version": "0.0.50",
  "description": "NVIDIA Jetson Nano IoT Central module",
  "main": "index.js",
  "scripts": {
    "test": "node ./node_modules/jest/bin/jest.js",
    "tslint": "node ./node_modules/tslint/bin/tslint -p ./tsconfig.json",
    "build": "node ./node_modules/typescript/bin/tsc -p .",
    "dockerbuild": "node ./scripts/dockerBuild.js --docker-build",
    "dockerpush": "node ./scripts/dockerBuild.js --docker-push",
    "preversion": "npm test",
    "version": "node ./scripts/dockerBuild.js --docker-build",
    "postversion": "node ./scripts/dockerBuild.js --docker-push",
    "postinstall": "node ./scripts/setupDevEnvironment.js"
  },
  "author": "sseiber",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:sseiber/nvidia-nano-airlift.git"
  },
  "dependencies": {
    "@hapi/boom": "^9.0.0",
    "@hapi/good": "^9.0.0",
    "@hapi/good-console": "^9.0.0",
    "@hapi/good-squeeze": "^6.0.0",
    "@hapi/hapi": "^19.0.2",
    "azure-iot-device": "^1.12.1",
    "azure-iot-device-mqtt": "^1.11.1",
    "fs-extra": "^8.1.0",
    "ip": "^1.1.5",
    "lodash.defaults": "^4.2.0",
    "lodash.get": "^4.4.2",
    "lodash.random": "^3.2.0",
    "lodash.set": "^4.3.2",
    "nconf": "^0.10.0",
    "pjson": "^1.0.9",
    "query-string": "^6.9.0",
    "request": "^2.88.0",
    "rimraf": "^3.0.0",
    "spryly": "^1.0.19",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@types/azure": "^0.9.20",
    "@types/jest": "^24.0.25",
    "@types/nconf": "0.10.0",
    "@types/node": "^13.1.4",
    "@types/request": "^2.48.4",
    "jest": "^24.9.0",
    "ts-jest": "^24.3.0",
    "tslint": "^5.20.1",
    "typescript": "^3.7.4"
  },
  "jest": {
    "rootDir": "./src/",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "transform": {
      ".tsx?": "ts-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "./__mocks__/fileMock.js"
    }
  }
}
